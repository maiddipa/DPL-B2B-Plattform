      ,[LicensePlate]
      ,[LicensePlateCountryId]
      --,[SubmissionId]
      --,[RowGuid]
      ,V.[CustomerReference]
      ,[ProcurementLogistics]
	  ,VP.LoadCarrierQuantity
	  ,LT.[Name] AS LoadCarrierTypeName
  FROM [dbo].[Vouchers] V
  INNER JOIN VoucherPosition VP ON V.Id = VP.VoucherId
  INNER JOIN LoadCarriers LC ON VP.LoadCarrierId = LC.Id
  INNER JOIN LoadCarrierTypes LT ON LC.TypeId = LT.Id
  INNER JOIN VoucherReasonTypes VR ON V.ReasonTypeId = VR.ID
  --LEFT JOIN ExpressCodes E ON V.ExpressCodeId = E.ID 
  INNER JOIN Documents D ON V.DocumentId = D.Id
  LEFT JOIN CustomerPartners Shipper ON V.ShipperId = Shipper.Id
  LEFT JOIN CustomerPartners Supplier ON V.SupplierId = Supplier.Id

  WHERE V.IsDeleted &lt;&gt; 1 AND VP.IsDeleted &lt;&gt; 1 AND V.CustomerDivisionId = ? AND V.[CreatedAt] &gt;= ? AND V.[CreatedAt] &lt;= ?</property>
