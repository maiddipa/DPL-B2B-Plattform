                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT V.[Id]
      --,[CreatedById]
      ,V.[CreatedAt]
      --,[ChangedById]
      --,[ChangedAt]
      --,[DeletedById]
      --,[DeletedAt]
      --,[IsDeleted]
      --,[ExpressCodeId]
	  --,E.DigitalCode 
